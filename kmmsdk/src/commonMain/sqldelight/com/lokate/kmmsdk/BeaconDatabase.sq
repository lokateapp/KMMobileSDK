CREATE TABLE Beacon(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT ,
    uuid TEXT NOT NULL,
    major INTEGER NOT NULL,
    minor INTEGER NOT NULL,
    range INTEGER NOT NULL,
    UNIQUE(uuid, major, minor)
);

removeAllBeacons:
DELETE FROM Beacon;

insertBeacon:
INSERT INTO Beacon(uuid,major,minor, range)
VALUES (?,?,?,?);

updateOrInsertBeacon:
INSERT OR REPLACE INTO Beacon(uuid,major,minor, range)
VALUES (?,?,?,?);

selectAllBeacons:
SELECT Beacon.*
FROM Beacon;